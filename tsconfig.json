{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": false,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2016",
    "resolveJsonModule": true,
    "lib": [
      "es2018",
      "dom"
    ],
    "allowSyntheticDefaultImports": true
  }
}
